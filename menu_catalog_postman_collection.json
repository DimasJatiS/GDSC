    {
      "name": "Create Menu",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{BASE_URL}}/menu",
          "host": [
            "{{BASE_URL}}"
          ],
          "path": [
            "menu"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Es Kopi Susu\",\n  \"category\": \"drinks\",\n  \"calories\": 180,\n  \"price\": 25000,\n  \"ingredients\": [\n    \"coffee\",\n    \"milk\",\n    \"ice\",\n    \"sugar\"\n  ],\n  \"description\": \"Classic iced coffee with milk\"\n}"
        }
      },
      "response": [],
      "event": [
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "pm.test(\"Status code is 201\", function () {",
              "    pm.response.to.have.status(201);",
              "});",
              "",
              "const json = pm.response.json();",
              "pm.test(\"Menu created and id exists\", function () {",
              "    pm.expect(json.data).to.have.property(\"id\");",
              "    pm.collectionVariables.set(\"menu_id\", json.data.id);",
              "});"
            ]
          }
        }
      ]
    },
